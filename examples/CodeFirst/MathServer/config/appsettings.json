{
  "GrpcServer": {
    //用于grpc启动后注册到服务发现的ip地址段
    "ServiceAddress": "0.0.0.0:886",
    //是否启用服务注册和服务发现,默认是true
    "EnableDiscovery": false,
    //服务发现服务器地址
    "DiscoveryUrl": "http://192.168.8.6:8500",
    //注册到服务发现的服务名称
    "DiscoveryServiceName": "Math.Test",
    //服务发现主动TTL的时间(秒)
    "DiscoveryTTLInterval": 10,
    //注册到服务发现的服务Tag
    "DiscoveryServiceTags": "v-1.0.0.1",
    //默认错误码
    "DefaultErrorCode": 4300000,
    //Jaeger配制(OpenTracing)
    "Jaeger": {
      //是否启用Jaeger,默认false
      "Enable": false,
      "AgentIp": "192.168.8.11",
      "AgentPort": 5775
    }
  }
}